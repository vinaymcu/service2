{  
   "$schema":"http://json-schema.org/draft-04/schema#",
   "type":"object",
   "additionalProperties":false,
   "javaInterfaces":[  
      "java.io.Serializable"
   ],
   "properties":{  
      "CreateConfigurationData":{  
         "type":"object",
         "additionalProperties":false,
         "properties":{  
            "MDCBitrate":{  
               "type":"integer"
            },
            "UseGlobalQoEBW":{  
               "type":"boolean"
            },
            "VideoTypeList":{  
               "$ref":"#/VideoTypeListDef"
            },
            "AvailableResolutionTypes":{  
               "$ref":"#/AvailableResolutionTypesDef"
            },
            "BWProfileList":{  
               "$ref":"#/BWProfileListDef"
            }
         },
         "required":[  
            "MDCBitrate",
            "VideoTypeList",
            "AvailableResolutionTypes",
            "BWProfileList"
         ]
      },
      "required":[  
         "CreateConfigurationData"
      ]
   },
   "AvailableResolutionTypesDef":{  
      "type":"array",
      "additionalItems":false,
      "items":{  
         "type":"object",
         "properties":{  
            "ResolutionTypeName":{  
               "type":"string",
               "maxLength":30
            }
         },
         "required":[  
            "ResolutionTypeName"
         ],
         "additionalProperties":false
      }
   },
   "BWProfileListDef":{  
      "type":"array",
      "additionalItems":false,
      "items":{  
         "type":"object",
         "properties":{  
            "BWProfileName":{  
               "type":"string",
               "maxLength":30
            },
            "Bandwidth":{  
               "type":"string",
               "pattern":"^[1-9][0-9]*$",
               "maxLength":55
            },
            " BWVideoTypeNameList ":{  
               "type":"array",
               "additionalItems":false,
               "items":{  
                  "type":"string",
                  "maxLength":70
               }
            }
           
         },
          "required":[  
               "BWProfileName",
               "Bandwidth",
               "BWVideoTypeNameList "
            ]
        } 
      },
      "VideoTypeListDef":{  
         "type":"array",
         "additionalItems":false,
         "items":{  
            "type":"object",
            "properties":{  
               "VideoTypeName":{  
                  "type":"string",
                  "maxLength":70
               },
               "BitRate":{  
                  "type":"string",
                  "pattern":"^[1-9][0-9]*$",
                  "maxLength":55
               },
               "ResolutionType":{  
                  "type":"string",
                  "maxLength":30
               }
            },
            "required":[  
               "VideoTypeName",
               "BitRate",
               "ResolutionType"
            ]
         }
      }
   }