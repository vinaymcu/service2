				"serialNumber": {
					"type": "string",
					"maxLength": 30
				},
				"deviceId": {
					"type": "string",
					"maxLength": 20
				},
				"ipAddress": {
					"type": "string",
					"maxLength": 20
				},
				"extIpAddress": {
					"type": "string",
					"maxLength": 20
				},
				"softwareVersion": {
					"type": "string",
